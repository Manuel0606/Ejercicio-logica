    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run api",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/ts-node-dev",
